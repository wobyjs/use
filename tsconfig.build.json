{
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "alwaysStrict": true,
        "declaration": true,
        "disableSizeLimit": true,
        "esModuleInterop": false,
        "forceConsistentCasingInFileNames": true,
        "inlineSourceMap": false,
        "inlineSources": false,
        // "isolatedModules": true,
        "jsx": "react-jsx",
        "jsxImportSource": "woby",
        "lib": [
            "dom",
            "esnext"
        ],
        "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
        "module": "es2020",
        "moduleResolution": "node",
        "newLine": "lf",
        "noEmitOnError": false,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noPropertyAccessFromIndexSignature": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "outDir": "./dist/",
        "declarationDir": "./dist/types/",
        "pretty": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "sourceMap": false,
        "strict": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "strictNullChecks": false,
        "target": "es2020",
        "useDefineForClassFields": false,
        "useUnknownInCatchVariables": true,
        "noImplicitAny": false,
        "noUncheckedIndexedAccess": false,
        "baseUrl": ".",
        "rootDir": "./src",
        "composite": true,
        "paths": {
            "woby/jsx-runtime": [
                "../../../woby/dist/jsx"
            ],
            "woby/jsx-runtime/*": [
                "../../../woby/dist/jsx/*"
            ],
        },
        "typeRoots": [
            // "./node_modules/oby/dist/types/types",
            // "./node_modules/oby/dist/types/objects",
            "./node_modules/woby/node_modules/oby/dist/types/types",
            "./node_modules/woby/node_modules/oby/dist/types/objects",
            "./node_modules/woby/dist/types"
        ],
        "types": [
            // "./node_modules/woby/dist/types/jsx/runtime",
            // "./node_modules/oby/types"
        ]
    },
    // "references": [
    //     {
    //         "path": "../woby"
    //     }
    // ],
    "exclude": [
        "node_modules/*",
        "**/*.test.ts*",
        "**/*.spec.ts*",
        "vite.config.ts",
        "vite.config.*.ts",
        "demo",
        "dist",
        "**/*.test.ts*",
        "**/*.demo.ts*",
        "**/tests.ts",
        "**/test.tsx",
    ]
    // "include": [
    //   "../woby/src/jsx"
    // ]
}